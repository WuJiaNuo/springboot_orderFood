CAZA;IAaA,CArDA;IAuDA;IACAC,GAxDA,iBAwDA;MACA;MACA;IACA,CA3DA;IA4DA;IACAC,IA7DA,gBA6DAP,EA7DA,EA6DA;MAAA;;MACA;MACAH;QACA;MACA,CAFA;IAGA,CAlEA;IAoEA;IACAW,YArEA,0BAqEA;MAAA;;MACA;QACA;UACA;YACA;UACA,CAFA,MAEA;YACA;UACA;QACA;MACA,CARA;IASA,CA/EA;IAiFA;IACAC,IAlFA,kBAkFA;MAAA;;MACAZ;QACA;;QACA;;QACA;MACA,CAJA;IAKA,CAxFA;IA0FA;IACAa,MA3FA,oBA2FA;MAAA;;MACAb;QACA;;QACA;;QACA;MACA,CAJA;IAKA;EAjGA;AAjDA","names":["components","ClassesSelect","data","listLoading","list","total","page","limit","searchObj","dialogVisible","student","saveBtnDisabled","dicItemList1000","rules","classId","required","message","trigger","realname","sex","age","created","console","mounted","methods","getClasses","selections","loadSysDicItem1000","sysDicItemApi","fetchData","api","resetData","removeDataById","id","confirmButtonText","cancelButtonText","type","then","catch","add","edit","saveOrUpdate","save","update"],"sourceRoot":"src/views/business","sources":["student.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search-div\">\n      <el-form label-width=\"70px\" size=\"small\">\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"学生昵称\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.name\" placeholder=\"学生昵称\"></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"姓名\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.realname\" placeholder=\"姓名\"></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"01男02女\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.sex\" placeholder=\"01男02女\"></el-input>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row style=\"display:flex\">\n          <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\n        </el-row>\n      </el-form>\n    </div>\n    <!-- 工具条 -->\n    <div class=\"tools-div\">\n      <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\">添 加</el-button>\n    </div>\n    <!-- 列表 -->\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      stripe\n      border\n      style=\"width: 100%;margin-top: 10px;\">\n\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n\n      <el-table-column sortale prop=\"id\" label=\"\"/>\n      <el-table-column sortale prop=\"classId\" label=\"所属班级\"/>\n      <el-table-column sortale prop=\"name\" label=\"学生昵称\"/>\n      <el-table-column sortale prop=\"realname\" label=\"姓名\"/>\n      <el-table-column sortale prop=\"sex\" label=\"01男02女\"/>\n      <el-table-column sortale prop=\"age\" label=\"年龄\"/>\n      <el-table-column sortale prop=\"createTime\" label=\"\"/>\n      <el-table-column sortale prop=\"updateTime\" label=\"\"/>\n\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <!-- 分页组件 -->\n    <el-pagination\n      :current-page=\"page\"\n      :total=\"total\"\n      :page-size=\"limit\"\n      style=\"padding: 30px 0; text-align: center;\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"fetchData\"\n    />\n\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"student\" :rules=\"rules\" label-width=\"100px\" size=\"small\"\n               style=\"padding-right: 40px;\">\n        <el-form-item label=\"所属班级\" prop=\"classId\">\n          <ClassesSelect\n            :value=\"student.id\"\n            @getInfo=\"getClasses\" :single=\"true\"/>\n\n        </el-form-item>\n        <el-form-item label=\"学生昵称\" prop=\"name\">\n          <el-input v-model=\"student.name\"/>\n        </el-form-item>\n        <el-form-item label=\"姓名\" prop=\"realname\">\n          <el-input v-model=\"student.realname\"/>\n        </el-form-item>\n\n        <el-form-item label=\"性别\" prop=\"sex\"  >\n          <el-radio v-model=\"student.sex\" v-for=\"l in dicItemList1000\" :label=\"l.code\" >{{l.name}}</el-radio>\n        </el-form-item>\n\n        <el-form-item label=\"年龄\" prop=\"age\">\n          <el-input-number :min=\"0\" v-model=\"student.age\" controls-position=\"right\"/>\n        </el-form-item>\n\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\n    import api from '@/api/business/student'\n    import sysDicItemApi from '@/api/system/sysDicItem'\n    import ClassesSelect from './studentClassesSelect'\n\n    const defaultForm = {}\n    export default {\n        components: {\n            ClassesSelect,\n        },\n        data() {\n            return {\n                listLoading: true, // 数据是否正在加载\n                list: null, // banner列表\n                total: 0, // 数据库中的总记录数\n                page: 1, // 默认页码\n                limit: 10, // 每页记录数\n                searchObj: {}, // 查询表单对象\n                dialogVisible: false,\n                student: defaultForm,\n                saveBtnDisabled: false,\n                dicItemList1000:null\n\n                , rules: {\n\n                    classId: [\n                        {required: true, message: '请输入所属班级', trigger: 'blur'}\n                    ]\n                    ,\n                    realname: [\n                        {required: true, message: '请输入姓名', trigger: 'blur'}\n                    ]\n                    ,\n                    sex: [\n                        {required: true, message: '请输入01男02女', trigger: 'blur'}\n                    ]\n                    ,\n                    age: [\n                        {required: true, message: '请输入年龄', trigger: 'blur'}\n                    ]\n\n                }\n            }\n        },\n        // 生命周期函数：内存准备完毕，页面尚未渲染\n        created() {\n            console.log('list created......')\n            this.fetchData()\n\n        },\n        // 生命周期函数：内存准备完毕，页面渲染成功\n        mounted() {\n            console.log('list mounted......')\n            this.loadSysDicItem1000();\n        },\n        methods: {\n            getClasses(selections) {\n                this.student.classId = selections.map(item => item.id).join(',')\n            },\n\n            loadSysDicItem1000(){\n                let queryObj=JSON.parse('{\"dicCode\":\"1000\"}')\n                sysDicItemApi.getAllSysDicItem(queryObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.dicItemList1000 = response.data;\n\n                    }\n                )\n            },\n\n            // 加载banner列表数据\n            fetchData(page = 1) {\n                debugger\n                this.page = page\n                api.getPageList(this.page, this.limit, this.searchObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.list = response.data.records\n                        this.total = response.data.total\n                        // 数据加载并绑定成功\n                        this.listLoading = false\n                    }\n                )\n            },\n\n            // 重置查询表单\n            resetData() {\n                console.log('重置查询表单')\n                this.searchObj = {}\n                this.fetchData()\n            },\n            // 根据id删除数据\n            removeDataById(id) {\n                // debugger\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => { // promise\n                    // 点击确定，远程调用ajax\n                    return api.removeById(id)\n                }).then((response) => {\n                    this.fetchData(this.page)\n                    this.$message.success(response.message || '删除成功')\n                }).catch(() => {\n                    this.$message.info('取消删除')\n                })\n            },\n\n            //弹出添加表单\n            add() {\n                this.dialogVisible = true\n                this.student = {}\n            },\n            //编辑\n            edit(id) {\n                this.dialogVisible = true\n                api.getById(id).then(response => {\n                    this.student = response.data\n                })\n            },\n\n            //添加或更新\n            saveOrUpdate() {\n                this.$refs.dataForm.validate(valid => {\n                    if (valid) {\n                        if (!this.student.id) {\n                            this.save()\n                        } else {\n                            this.update()\n                        }\n                    }\n                })\n            },\n\n            //添加\n            save() {\n                api.save(this.student).then(response => {\n                    this.$message.success('操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            },\n\n            //更新\n            update() {\n                api.updateById(this.student).then(response => {\n                    this.$message.success(response.message || '操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            }\n        }\n    }\n</script>\n"]}]}
