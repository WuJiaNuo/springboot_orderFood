ACAL;QACA;MACA,CAFA;IAGA,CA7DA;IA8DA;IACAa,YA/DA,0BA+DA;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;IACA,CArEA;IAsEA;IACAC,IAvEA,kBAuEA;MAAA;;MACAd;QACA;;QACA;;QACA;MACA,CAJA;IAKA,CA7EA;IA8EA;IACAe,MA/EA,oBA+EA;MAAA;;MACAf;QACA;;QACA;;QACA;MACA,CAJA;IAKA;EArFA;AA3BA","names":["avatar","components","ImageCropper","PanThumb","data","listLoading","list","total","page","limit","searchObj","dialogVisible","eduTeacher","saveBtnDisabled","imagecropperShow","imagecropperKey","BASE_API","created","console","mounted","methods","fetchData","api","close","cropSuccess","resetData","removeDataById","id","confirmButtonText","cancelButtonText","type","then","catch","add","edit","saveOrUpdate","save","update"],"sourceRoot":"src/views/business","sources":["eduTeacher.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search-div\">\n      <el-form label-width=\"70px\" size=\"small\">\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"名称\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.name\" placeholder=\"名称\"></el-input>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row style=\"display:flex\">\n          <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\n        </el-row>\n      </el-form>\n    </div>\n    <!-- 工具条 -->\n    <div class=\"tools-div\">\n      <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\">添 加</el-button>\n    </div>\n    <!-- 列表 -->\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      stripe\n      border\n      style=\"width: 100%;margin-top: 10px;\">\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"name\" label=\"讲师姓名\"/>\n      <el-table-column prop=\"intro\" label=\"讲师简介\"/>\n      <el-table-column prop=\"career\" label=\"讲师资历,一句话说明讲师\"/>\n      <el-table-column prop=\"level\" label=\"头衔 1高级讲师 2首席讲师\"/>\n      <el-table-column prop=\"avatar\" label=\"讲师头像\"/>\n      <el-table-column prop=\"sort\" label=\"排序\"/>\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页组件 -->\n    <el-pagination\n      :current-page=\"page\"\n      :total=\"total\"\n      :page-size=\"limit\"\n      style=\"padding: 30px 0; text-align: center;\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"fetchData\"\n    />\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"eduTeacher\" label-width=\"100px\" size=\"small\" style=\"padding-right: 40px;\">\n        <el-form-item label=\"讲师姓名\" prop=\"name\">\n          <el-input v-model=\"eduTeacher.name\"/>\n        </el-form-item>\n        <el-form-item label=\"讲师排序\">\n          <el-input-number v-model=\"eduTeacher.sort\" controls-position=\"right\" :min=\"0\"/>\n        </el-form-item>\n        <el-form-item label=\"头衔\" prop=\"level\">\n          <el-select v-model=\"eduTeacher.level\" clearable placeholder=\"请选择\">\n            <el-option :value=\"1\" label=\"高级讲师\"/>\n            <el-option :value=\"2\" label=\"首席讲师\"/>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"讲师头像\" prop=\"avatar\">\n          <!-- 头衔缩略图 -->\n          <pan-thumb :image=\"eduTeacher.avatar\"/>\n          <!-- 文件上传按钮 -->\n          <el-button type=\"primary\" icon=\"el-icon-upload\" @click=\"imagecropperShow=true\">更换头像\n          </el-button>\n          <!--\n      v-show：是否显示上传组件\n      :key：类似于id，如果一个页面多个图片上传控件，可以做区分\n      :url：后台上传的url地址\n      @close：关闭上传组件\n      @crop-upload-success：上传成功后的回调\n          <input type=\"file\" name=\"file\"/>\n      -->\n          <image-cropper\n            v-show=\"imagecropperShow\"\n            :width=\"300\"\n            :height=\"300\"\n            :key=\"imagecropperKey\"\n            :url=\"BASE_API+'/eduoss/fileoss/upload'\"\n            field=\"file\"\n            @close=\"close\"\n            @crop-upload-success=\"cropSuccess\"/>\n        </el-form-item>\n        <el-form-item label=\"讲师资历\" prop=\"career\">\n          <el-input v-model=\"eduTeacher.career\"/>\n        </el-form-item>\n        <el-form-item label=\"讲师简介\" prop=\"intro\">\n          <el-input v-model=\"eduTeacher.intro\" :rows=\"10\" type=\"textarea\"/>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import api from '@/api/business/eduTeacher'\n    import ImageCropper from '@/components/ImageCropper'\n    import PanThumb from '@/components/PanThumb'\n\n    const defaultForm = {\n        avatar: ''\n    }\n    export default {\n        components: {ImageCropper, PanThumb},\n        data() {\n            return {\n                listLoading: true, // 数据是否正在加载\n                list: null, // banner列表\n                total: 0, // 数据库中的总记录数\n                page: 1, // 默认页码\n                limit: 5, // 每页记录数\n                searchObj: {}, // 查询表单对象\n                dialogVisible: false,\n                eduTeacher: defaultForm,\n                saveBtnDisabled: false,\n                imagecropperShow: false,\n                imagecropperKey: 0,//上传组件key值\n                BASE_API: process.env.VUE_APP_BASE_API, //获取dev.env.js里面地址\n            }\n        },\n        // 生命周期函数：内存准备完毕，页面尚未渲染\n        created() {\n            console.log('list created......')\n            this.fetchData()\n        },\n        // 生命周期函数：内存准备完毕，页面渲染成功\n        mounted() {\n            console.log('list mounted......')\n        },\n        methods: {\n            // 加载banner列表数据\n            fetchData(page = 1) {\n                debugger\n                this.page = page\n                api.getPageList(this.page, this.limit, this.searchObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.list = response.data.records\n                        this.total = response.data.total\n                        // 数据加载并绑定成功\n                        this.listLoading = false\n                    }\n                )\n            },\n            close() { //关闭上传弹框的方法\n                this.imagecropperShow = false\n                //上传组件初始化\n                this.imagecropperKey = this.imagecropperKey + 1\n            },\n            //上传成功方法\n            cropSuccess(data) {\n                this.imagecropperShow = false\n                //上传之后接口返回图片地址\n                this.eduTeacher.avatar = data.url\n                this.imagecropperKey = this.imagecropperKey + 1\n            },\n            // 重置查询表单\n            resetData() {\n                console.log('重置查询表单')\n                this.searchObj = {}\n                this.fetchData()\n            },\n            // 根据id删除数据\n            removeDataById(id) {\n                // debugger\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => { // promise\n                    // 点击确定，远程调用ajax\n                    return api.removeById(id)\n                }).then((response) => {\n                    this.fetchData(this.page)\n                    this.$message.success(response.message || '删除成功')\n                }).catch(() => {\n                    this.$message.info('取消删除')\n                })\n            },\n            //弹出添加表单\n            add() {\n                this.dialogVisible = true\n                this.eduTeacher = {avatar: ''}\n            },\n            //编辑\n            edit(id) {\n                this.dialogVisible = true\n                api.getById(id).then(response => {\n                    this.eduTeacher = response.data\n                })\n            },\n            //添加或更新\n            saveOrUpdate() {\n                if (!this.eduTeacher.id) {\n                    this.save()\n                } else {\n                    this.update()\n                }\n            },\n            //添加\n            save() {\n                api.save(this.eduTeacher).then(response => {\n                    this.$message.success('操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            },\n            //更新\n            update() {\n                api.updateById(this.eduTeacher).then(response => {\n                    this.$message.success(response.message || '操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            }\n        }\n    }\n</script>\n"]}]}
