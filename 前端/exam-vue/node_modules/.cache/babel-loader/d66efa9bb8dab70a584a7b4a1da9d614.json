],"sourcesContent":["<template>\n    <div>\n        <el-input placeholder=\"请选择\" :size=\"size\" :disabled=\"inpDisabled\" style=\"line-hight:40px\" v-model=\"name\" class=\"input-with-select\">\n            <el-button slot=\"append\" :disabled=\"btnDisabled\" @click=\"showUserSelect\" icon=\"el-icon-search\"></el-button>\n        </el-input>\n        <TestClassesSelectDialog\n                ref=\"testClassesSelect\"\n                @doSubmit=\"selectionsToInput\"\n                :selectData=\"selectData\"\n                 :where=\"where\"\n        />\n    </div>\n</template>\n<script>\n    import TestClassesSelectDialog from './testStudentTestClassesSelectOneDialog'\n    import testClassesApi from '@/api/cust/testClasses'\n    export default {\n        data () {\n            return {\n                name: '',\n                selectData: {\n                    type: Object,\n                    default: () => {\n                        return {};\n                    },\n                },\n            }\n        },\n        props: {\n            size: {\n                type: String,\n                default: 'small'\n            },\n            myvals: {\n                type: String,\n                default: ''\n            },\n            btnDisabled: {\n                type: Boolean,\n                default: false\n            },\n            inpDisabled: {\n                type: Boolean,\n                default: true\n            },\n            where: {  // 查询条件\n                type: String,\n                default: ''\n            }\n        },\n        components: {\n            TestClassesSelectDialog\n        },\n        created () {\n        },\n        watch: {\n            myvals: {\n                handler (newVal) {\n                    this.selectData = {}\n                    if (newVal && newVal.length > 0) {\n                        testClassesApi.getById(newVal).then(\n                            response => {\n                                if (response && response.data) {\n                                    this.selectData = response.data\n                                }\n                            }\n                        )\n                    }\n                },\n                immediate: true,\n                deep: false\n            },\n            selectData: {\n                handler (newVal) {\n                    this.name = newVal.name\n                },\n                immediate: false,\n                deep: false\n            }\n        },\n        methods: {\n            // 设置选中\n            selectionsToInput (selections) {\n                this.selectData = selections\n                this.$emit('getInfo', this.selectData.id)\n            },\n            // 显示列表\n            showUserSelect () {\n                this.$refs.testClassesSelect.init()\n            }\n        }\n    }\n</script>\n<style>\n    .el-form-item__content .el-input-group {\n        vertical-align: middle;\n    }\n    .el-tag + .el-tag {\n        margin-left: 5px;\n        margin-bottom: 5px;\n    }\n</style>\n"]}]}
