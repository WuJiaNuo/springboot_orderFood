;;QACA;;QACA;MACA,CAJA;IAKA;EA/LA;AA1EA","names":["components","TestClassesSelectOne","TestStudentCourseSelect","TestStudentCourseView","SelectDeptTree","data","listLoading","list","total","page","limit","searchObj","dialogVisible","testStudent","saveBtnDisabled","selectValue","sysDicItemXdicCode1000List","dataDeptTree","rules","classId","required","message","trigger","realname","sexCode","age","deptId","searchDeptValue","defaultDeptExpandedKeys","selectedDeptValue","selectedDeptName","watch","value","created","console","mounted","methods","getRowKeys","row","handleDeptTreeSelectChange","filterDeptNode","sizeChangeHandle","val","getTestClassesXbe70ea7b","selections","getAllSysDicItemXdicCode1000","queryObj","sysDicItemApi","then","getTestStudentCourseX9d1ca1ae","saveDeptId","treeId","treeName","fetchPopDept","sysDeptApi","fetchData","api","resetData","removeDataById","id","confirmButtonText","cancelButtonText","type","catch","handleSelectionChange","selection","batchRemove","idList","add","edit","saveOrUpdate","save","update"],"sourceRoot":"src/views/pages/cust","sources":["testStudent.vue"],"sourcesContent":["<template>\r\n        <el-container style=\"height: 100%; border: 1px solid #eee\">\r\n        <el-aside width=\"200px\" style=\"background-color: rgb(238, 241, 246)\">\r\n            <el-input v-model=\"searchDeptValue\" placeholder=\"请输入关键字过滤\" style=\"margin-bottom:10px;\"/>\r\n            <el-tag>当前选择：{{selectedDeptName}}</el-tag>\r\n            <el-tree\r\n                    ref=\"eltree\"\r\n                    :data=\"dataDeptTree\"\r\n                    node-key=\"id\"\r\n                    :default-expand-all=\"true\"\r\n                    :expand-on-click-node=\"false\"\r\n                    :default-expanded-keys=\"defaultDeptExpandedKeys\"\r\n                    :filter-node-method=\"filterDeptNode\"\r\n                    @current-change=\"handleDeptTreeSelectChange\"\r\n            />\r\n        </el-aside>\r\n  <el-container>\r\n   <el-main>\r\n    <div class=\"app-container\">\r\n        <div class=\"search-div\">\r\n            <el-form label-width=\"70px\" size=\"small\">\r\n                <el-row>\r\n                            <el-col :span=\"8\">\r\n                                <el-form-item label=\"所属班级\">\r\n                                        <el-input v-model=\"searchObj.className\"\r\n                                                  placeholder=\"班级名称\" clearable/>\r\n                                </el-form-item>\r\n                            </el-col>\r\n                            <el-col :span=\"8\">\r\n                                <el-form-item label=\"学生昵称\">\r\n                                        <el-input v-model=\"searchObj.name\" placeholder=\"学生昵称\" clearable/>\r\n                                </el-form-item>\r\n                            </el-col>\r\n                            <el-col :span=\"8\">\r\n                                <el-form-item label=\"姓名\">\r\n                                        <el-input v-model=\"searchObj.realname\" placeholder=\"姓名\" clearable/>\r\n                                </el-form-item>\r\n                            </el-col>\r\n                            <el-col :span=\"8\">\r\n                                <el-form-item label=\"性别\">\r\n                                        <el-radio v-model=\"searchObj.sexCode\"\r\n                                                  v-for=\"l in sysDicItemXdicCode1000List\"\r\n                                                  :label=\"l.code\">{{l.name}}\r\n                                        </el-radio>\r\n                                </el-form-item>\r\n                            </el-col>\r\n                            <el-col :span=\"8\">\r\n                                <el-form-item label=\"所属部门\">\r\n                                        <el-input v-model=\"searchObj.deptName\"\r\n                                                  placeholder=\"部门名称\" clearable/>\r\n                                </el-form-item>\r\n                            </el-col>\r\n                </el-row>\r\n                <el-row style=\"display:flex\">\r\n                    <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\r\n                    <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\r\n                </el-row>\r\n            </el-form>\r\n        </div>\r\n        <!-- 工具条 -->\r\n        <div class=\"tools-div\">\r\n            <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\" :disabled=\"$hasBP('bnt.testStudent.add')  === false\">添 加</el-button>\r\n            <el-button class=\"btn-add\" size=\"mini\" @click=\"batchRemove()\" :disabled=\"$hasBP('bnt.testStudent.remove')  === false\">批量删除</el-button>\r\n        </div>\r\n        <!-- 列表 -->\r\n        <el-table\r\n                v-loading=\"listLoading\"\r\n                :data=\"list\"\r\n                :row-key=\"getRowKeys\"\r\n                stripe\r\n                border\r\n                style=\"width: 100%;margin-top: 10px;\"\r\n                @selection-change=\"handleSelectionChange\">\r\n            <el-table-column\r\n                    label=\"序号\"\r\n                    width=\"70\"\r\n                    align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    {{ (page - 1) * limit + scope.$index + 1 }}\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column type=\"selection\"  :reserve-selection=\"true\"/>\r\n                            <el-table-column sortable prop=\"className\" label=\"班级名称\"/>\r\n                        <el-table-column sortable prop=\"name\" label=\"学生昵称\"/>\r\n                        <el-table-column sortable prop=\"realname\" label=\"姓名\"/>\r\n                            <el-table-column sortable prop=\"sexName\" label=\"性别\"/>\r\n                        <el-table-column sortable prop=\"age\" label=\"年龄\"/>\r\n                        <el-table-column sortable prop=\"createTime\" label=\"创建时间\"/>\r\n                        <el-table-column sortable prop=\"updateTime\" label=\"修改时间\"/>\r\n                            <el-table-column sortable prop=\"deptName\" label=\"部门名称\"/>\r\n                                <el-table-column type=\"expand\" label=\"...\">\r\n                                    <template slot-scope=\"props\">\r\n                                        <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n                                                        <el-form-item label=\"班级名称\">\r\n                                                            <span>{{ props.row.className }}</span>\r\n                                                        </el-form-item>\r\n                                                    <el-form-item label=\"学生昵称\">\r\n                                                        <span>{{ props.row.name }}</span>\r\n                                                    </el-form-item>\r\n                                                    <el-form-item label=\"姓名\">\r\n                                                        <span>{{ props.row.realname }}</span>\r\n                                                    </el-form-item>\r\n                                                        <el-form-item label=\"性别\">\r\n                                                            <span>{{ props.row.sexName }}</span>\r\n                                                        </el-form-item>\r\n                                                    <el-form-item label=\"年龄\">\r\n                                                        <span>{{ props.row.age }}</span>\r\n                                                    </el-form-item>\r\n                                                    <el-form-item label=\"创建时间\">\r\n                                                        <span>{{ props.row.createTime }}</span>\r\n                                                    </el-form-item>\r\n                                                    <el-form-item label=\"修改时间\">\r\n                                                        <span>{{ props.row.updateTime }}</span>\r\n                                                    </el-form-item>\r\n                                                        <el-form-item label=\"部门名称\">\r\n                                                            <span>{{ props.row.deptName }}</span>\r\n                                                        </el-form-item>\r\n                                                <el-form-item label=\"课程名称\">\r\n                                                        <span><TestStudentCourseView :studentId=\"props.row.id\"></TestStudentCourseView></span>\r\n                                                  </el-form-item>\r\n                                        </el-form>\r\n                                    </template>\r\n                                </el-table-column>\r\n            <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" :disabled=\"$hasBP('bnt.testStudent.update')  === false\" title=\"修改\"/>\r\n                    <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\"\r\n                               :disabled=\"$hasBP('bnt.testStudent.remove')  === false\"     title=\"删除\"/>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <!-- 分页组件 -->\r\n        <el-pagination\r\n                @size-change=\"sizeChangeHandle\"\r\n                :current-page=\"page\"\r\n                :total=\"total\"\r\n                :page-size=\"limit\"\r\n                style=\"padding: 30px 0; text-align: center;\"\r\n                :page-sizes=\"[5, 10, 50, 100]\"\r\n                layout=\"total,sizes, prev, pager, next, jumper\"\r\n                @current-change=\"fetchData\"\r\n        />\r\n        <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\">\r\n            <el-form ref=\"dataForm\" :model=\"testStudent\" :rules=\"rules\" label-width=\"100px\" size=\"small\"\r\n                     style=\"padding-right: 40px;\">\r\n                    <el-form-item label=\"所属班级\" prop=\"classId\">\r\n                                    <TestClassesSelectOne\r\n                                            :myvals=\"testStudent.classId\"\r\n                                            @getInfo=\"getTestClassesXbe70ea7b\" where=\"\"  />\r\n                    </el-form-item>\r\n                    <el-form-item label=\"学生昵称\" prop=\"name\">\r\n                                <el-input v-model=\"testStudent.name\" clearable/>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"姓名\" prop=\"realname\">\r\n                                <el-input v-model=\"testStudent.realname\" clearable/>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"性别\" prop=\"sexCode\">\r\n                            <el-radio v-model=\"testStudent.sexCode\" v-for=\"l in sysDicItemXdicCode1000List\" :label=\"l.code\" >{{l.name}}</el-radio>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"年龄\" prop=\"age\">\r\n                                <el-input-number :min=\"0\" v-model=\"testStudent.age\" controls-position=\"right\"  clearable/>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"所属部门\" prop=\"deptId\">\r\n                                           <select-dept-tree @saveTreeId=\"saveDeptId\" treeTitle=\"选择部门\" :isLazy=\"false\" :dataTree=\"dataDeptTree\"\r\n                                                             :treeVal=\"testStudent.deptName\" :treeId=\"testStudent.deptId\"></select-dept-tree>\r\n                    </el-form-item>\r\n                            <el-form-item label=\"选择课程\" prop=\"courseId\">\r\n                                        <TestStudentCourseSelect\r\n                                                :myvals=\"testStudent.courseIdList\"\r\n                                                @getInfo=\"getTestStudentCourseX9d1ca1ae\" :single=\"false\" where=\"\"   />\r\n                            </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\r\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\r\n      </span>\r\n        </el-dialog>\r\n    </div>\r\n            </el-main>\r\n        </el-container>\r\n    </el-container>\r\n</template>\r\n<script>\r\n    import api from '@/api/cust/testStudent'\r\n    import TestClassesSelectOne from './testStudentTestClassesSelectOne'\r\n    import sysDicItemApi from '@/api/system/sysDicItem'\r\n    import sysDeptApi from '@/api/system/sysDept'\r\n    import TestStudentCourseSelect from './testStudentTestStudentCourseSelect'\r\n    import TestStudentCourseView from './testStudentTestStudentCourseView'\r\n\r\n    const defaultForm = {\r\n    }\r\n    export default {\r\n        components: {\r\n            TestClassesSelectOne,\r\n            TestStudentCourseSelect,\r\n            TestStudentCourseView,\r\n\r\n            SelectDeptTree: () => import('@/components/Custom/TreeSelect/TreeSelect')\r\n        },\r\n        data() {\r\n            return {\r\n                listLoading: true, // 数据是否正在加载\r\n                list: [], // banner列表\r\n                total: 0, // 数据库中的总记录数\r\n                page: 1, // 默认页码\r\n                limit: 5, // 每页记录数\r\n                searchObj: {}, // 查询表单对象\r\n                dialogVisible: false,\r\n                testStudent:defaultForm,\r\n                saveBtnDisabled:false,\r\n                selectValue:[] //复选框选择内容封装数组\r\n                , sysDicItemXdicCode1000List:[]\r\n                , dataDeptTree:[]//树形选择数据\r\n                , rules:{\r\n                \r\n                classId:\r\n                [\r\n                    {required: true, message: '请输入所属班级', trigger: 'blur'}\r\n                ]\r\n                ,\r\n                realname:\r\n                [\r\n                    {required: true, message: '请输入姓名', trigger: 'blur'}\r\n                ]\r\n                ,\r\n                sexCode:\r\n                [\r\n                    {required: true, message: '请输入性别', trigger: 'blur'}\r\n                ]\r\n                ,\r\n                age:\r\n                [\r\n                    {required: true, message: '请输入年龄', trigger: 'blur'}\r\n                ]\r\n                ,\r\n                deptId:\r\n                [\r\n                    {required: true, message: '请输入所属部门', trigger: 'blur'}\r\n                ]\r\n            }\r\n            //左侧树begin\r\n              ,searchDeptValue: '', //左侧部门搜索绑定\r\n              defaultDeptExpandedKeys: [],\r\n              selectedDeptValue: '',\r\n              selectedDeptName: ''\r\n            //左侧树end\r\n        }\r\n        },\r\n        watch: {\r\n            //监听左侧树查询事件\r\n            searchDeptValue(value) {\r\n                this.$refs.eltree.filter(value)\r\n            }\r\n        },\r\n        // 生命周期函数：内存准备完毕，页面尚未渲染\r\n        created() {\r\n            console.log('list created......')\r\n            this.fetchData()\r\n            this.getAllSysDicItemXdicCode1000()\r\n            this.fetchPopDept()\r\n        },\r\n        // 生命周期函数：内存准备完毕，页面渲染成功\r\n        mounted() {\r\n            console.log('list mounted......')\r\n        },\r\n        methods: {\r\n            getRowKeys(row) {\r\n                return row.id\r\n            },\r\n            //左侧树形方法begin\r\n            // 树节点选中改变\r\n            handleDeptTreeSelectChange(data) {\r\n                this.selectedDeptValue = data.id\r\n                if(data.label){\r\n                    this.selectedDeptName = data.label\r\n                }else{\r\n                    this.selectedDeptName = data.name\r\n                }\r\n                //重新加载主数据\r\n                this.searchObj.deptId = data.id\r\n                this.fetchData(this.page)\r\n            },\r\n            filterDeptNode(value, data) {\r\n                if (!value) return true\r\n                if(data.label){\r\n                    this.selectedDeptName = data.label\r\n                }else{\r\n                    this.selectedDeptName = data.name\r\n                }\r\n            },\r\n            //左侧树形方法end\r\n            // 每页数\r\n            sizeChangeHandle(val) {\r\n                this.limit = val;\r\n                this.page = 1;\r\n                this.fetchData(this.page);\r\n            },\r\n            getTestClassesXbe70ea7b(selections) {\r\n                this.testStudent.classId = selections\r\n                this.$refs[\"dataForm\"].clearValidate(\"classId\")\r\n                this.rules.classId = []\r\n            },\r\n            getAllSysDicItemXdicCode1000() {\r\n                let queryObj = {}\r\n                 queryObj = JSON.parse('{\"dicCode\":\"1000\"}')\r\n                sysDicItemApi.getAllSysDicItem(queryObj)\r\n                    .then(response => {\r\n                        this.sysDicItemXdicCode1000List = response.data\r\n                    })\r\n            },\r\n            getTestStudentCourseX9d1ca1ae(selections) {\r\n                this.testStudent.courseIdList = selections\r\n            },\r\n            //回调保存选择的部门id\r\n            saveDeptId(treeId, treeName) {\r\n                this.testStudent.deptId = treeId\r\n                this.$refs[\"dataForm\"].clearValidate(\"deptId\")\r\n                this.rules.deptId = []\r\n            },\r\n            fetchPopDept() {\r\n                //加载弹出选择部门数据\r\n                sysDeptApi.findSelectNodes().then(response => {\r\n                    this.dataDeptTree = response.data\r\n                })\r\n            },\r\n            // 加载banner列表数据\r\n            fetchData(page = 1) {\r\n                debugger\r\n                this.page = page\r\n                api.getPageList(this.page, this.limit, this.searchObj).then(\r\n                    response => {\r\n                        //this.list = response.data.list\r\n                        this.list = response.data.records\r\n                        this.total = response.data.total\r\n                        // 数据加载并绑定成功\r\n                        this.listLoading = false\r\n                    }\r\n                )\r\n            },\r\n            // 重置查询表单\r\n            resetData() {\r\n                console.log('重置查询表单')\r\n                this.searchObj = {}\r\n                this.fetchData()\r\n            },\r\n            // 根据id删除数据\r\n            removeDataById(id) {\r\n                // debugger\r\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => { // promise\r\n                    // 点击确定，远程调用ajax\r\n                    return api.removeById(id)\r\n                }).then((response) => {\r\n                    this.fetchData(this.page)\r\n                    this.$message.success(response.message || '删除成功')\r\n                }).catch(() => {\r\n                    this.$message.info('取消删除')\r\n                })\r\n            },\r\n            //复选框发生变化执行方法\r\n            handleSelectionChange(selection) {\r\n                this.selectValue = selection\r\n                //console.log(this.selectValue)\r\n            },\r\n            //批量删除\r\n            batchRemove() {\r\n                //判断\r\n                if (this.selectValue.length == 0) {\r\n                    this.$message.warning('请选择要删除的记录！')\r\n                    return\r\n                }\r\n                this.$confirm('此操作将永久删除, 是否继续?', '提示', {\r\n                    confirmButtonText: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                }).then(() => {\r\n                    //数组\r\n                    var idList = []\r\n                    //获取多个复选框对应id，封装到数组里面\r\n                    // [1,2,3]\r\n                    for (var i = 0; i < this.selectValue.length; i++) {\r\n                        var obj = this.selectValue[i]\r\n                        //id值\r\n                        var id = obj.id\r\n                        //放到数组里面\r\n                        idList.push(id)\r\n                    }\r\n                    api.batchRemove(idList).then(response => {\r\n                        //提示\r\n                        this.$message({\r\n                            type: 'success',\r\n                            message: '删除成功!'\r\n                        });\r\n                        //刷新\r\n                        this.fetchData()\r\n                    })\r\n                })\r\n            },\r\n            //弹出添加表单\r\n            add() {\r\n                this.dialogVisible = true\r\n                this.testStudent = {\r\n                }\r\n                this.rules.classId = [{required: true, message: '请输入所属班级', trigger: 'blur'}]\r\n                this.rules.realname = [{required: true, message: '请输入姓名', trigger: 'blur'}]\r\n                this.rules.sexCode = [{required: true, message: '请输入性别', trigger: 'blur'}]\r\n                this.rules.age = [{required: true, message: '请输入年龄', trigger: 'blur'}]\r\n                this.rules.deptId = [{required: true, message: '请输入所属部门', trigger: 'blur'}]\r\n            },\r\n            //编辑\r\n            edit(id) {\r\n                this.dialogVisible = true\r\n                this.rules.classId = [{required: true, message: '请输入所属班级', trigger: 'blur'}]\r\n                this.rules.realname = [{required: true, message: '请输入姓名', trigger: 'blur'}]\r\n                this.rules.sexCode = [{required: true, message: '请输入性别', trigger: 'blur'}]\r\n                this.rules.age = [{required: true, message: '请输入年龄', trigger: 'blur'}]\r\n                this.rules.deptId = [{required: true, message: '请输入所属部门', trigger: 'blur'}]\r\n                api.getById(id).then(response => {\r\n                    this.$nextTick(function () {\r\n                        this.testStudent = response.data\r\n                        if(this.$refs.eltreeDept){\r\n                            this.$refs.eltreeDept.setCheckedKeys(this.testStudent.deptIdList);\r\n                        }\r\n                    })\r\n                })\r\n            },\r\n            //添加或更新\r\n            saveOrUpdate() {\r\n                this.$refs.dataForm.validate(valid => {\r\n                    if (valid) {\r\n                        if (!this.testStudent.id) {\r\n                            this.save()\r\n                        } else {\r\n                            this.update()\r\n                        }\r\n                    }\r\n                })\r\n            },\r\n            //添加\r\n            save() {\r\n                api.save(this.testStudent).then(response => {\r\n                    this.$message.success('操作成功')\r\n                    this.dialogVisible = false\r\n                    this.fetchData(this.page)\r\n                })\r\n            },\r\n            //更新\r\n            update() {\r\n                api.updateById(this.testStudent).then(response => {\r\n                    this.$message.success(response.message || '操作成功')\r\n                    this.dialogVisible = false\r\n                    this.fetchData(this.page)\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .demo-table-expand {\r\n        font-size: 0;\r\n    }\r\n    .demo-table-expand label {\r\n        width: 90px;\r\n        color: #99a9bf;\r\n    }\r\n    .demo-table-expand .el-form-item {\r\n        margin-right: 0;\r\n        margin-bottom: 0;\r\n        width: 50%;\r\n    }\r\n</style>"]}]}
