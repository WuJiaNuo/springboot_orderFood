;QACA;QACA;QACA,mCAHA,CAKA;;QACA;MACA,CARA;IAUA,CA1EA;IA2EA;IACAE,SA5EA,uBA4EA;MACAhB;MACA;MACA;IACA,CAhFA;IAiFA;IACAiB,cAlFA,0BAkFAC,EAlFA,EAkFA;MAAA;;MACA;MACA;QACAC,uBADA;QAEAC,sBAFA;QAGAC;MAHA,GAIAd,IAJA,CAIA;QAAA;QACA;QACA;MACA,CAPA,EAOAA,IAPA,CAOA;QACA;;QACA;MACA,CAVA,EAUAC,KAVA,CAUA;QACA;MACA,CAZA;IAaA,CAjGA;IAkGA;IACAc,GAnGA,iBAmGA;MACA;MACA;QAAA7B;MAAA;IACA,CAtGA;IAuGA;IACA8B,IAxGA,gBAwGAL,EAxGA,EAwGA;MAAA;;MACA;MACAJ;QACA;;QACA;MACA,CAHA;IAIA,CA9GA;IA+GA;IACAU,YAhHA,0BAgHA;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;IACA,CAtHA;IAuHA;IACAC,IAxHA,kBAwHA;MAAA;;MACAX;QACA;;QACA;;QACA;MACA,CAJA;IAKA,CA9HA;IA+HA;IACAY,MAhIA,oBAgIA;MAAA;;MACAZ;QACA;;QACA;;QACA;MACA,CAJA;IAKA;EAtIA;AAjCA","names":["data","listLoading","list","total","page","limit","searchObj","dialogVisible","eduCourse","cover","saveBtnDisabled","BASE_API","teacherList","subjectOneList","subjectTwoList","created","console","mounted","methods","uploadLogo","param","formData","oss","then","catch","subjectLevelOneChanged","value","getOneSubject","subject","getListTeacher","api","fetchData","resetData","removeDataById","id","confirmButtonText","cancelButtonText","type","add","edit","saveOrUpdate","save","update"],"sourceRoot":"src/views/business","sources":["eduCourse.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search-div\">\n      <el-form label-width=\"70px\" size=\"small\">\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"名称\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.title\" placeholder=\"标题\"></el-input>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row style=\"display:flex\">\n          <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\n        </el-row>\n      </el-form>\n    </div>\n    <!-- 工具条 -->\n    <div class=\"tools-div\">\n      <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\">添 加</el-button>\n    </div>\n    <!-- 列表 -->\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      stripe\n      border\n      style=\"width: 100%;margin-top: 10px;\">\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"title\" label=\"课程标题\"/>\n      <el-table-column prop=\"price\" label=\"课程销售价格\"/>\n      <el-table-column prop=\"lessonNum\" label=\"总课时\"/>\n      <el-table-column prop=\"buyCount\" label=\"销售数量\"/>\n      <el-table-column prop=\"viewCount\" label=\"浏览数量\"/>\n      <el-table-column prop=\"status\" label=\"课程状态\"/>\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页组件 -->\n    <el-pagination\n      :current-page=\"page\"\n      :total=\"total\"\n      :page-size=\"limit\"\n      style=\"padding: 30px 0; text-align: center;\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"fetchData\"\n    />\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"eduCourse\" label-width=\"100px\" size=\"small\" style=\"padding-right: 40px;\">\n        <el-form-item label=\"课程讲师ID\" prop=\"teacherId\">\n          <el-select\n            v-model=\"eduCourse.teacherId\"\n            placeholder=\"请选择\">\n            <el-option\n              v-for=\"teacher in teacherList\"\n              :key=\"teacher.id\"\n              :label=\"teacher.name\"\n              :value=\"teacher.id\"/>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"课程分类\" prop=\"subjectId\">\n          <el-select\n            v-model=\"eduCourse.subjectParentId\"\n            placeholder=\"一级分类\" @change=\"subjectLevelOneChanged\">\n            <el-option\n              v-for=\"subject2 in subjectOneList\"\n              :key=\"subject2.id\"\n              :label=\"subject2.name\"\n              :value=\"subject2.id\"/>\n          </el-select>\n          <!-- 二级分类 -->\n          <el-select v-model=\"eduCourse.subjectId\" @change=\"$forceUpdate()\" placeholder=\"二级分类\">\n            <el-option\n              v-for=\"subject in subjectTwoList\"\n              :key=\"subject.id\"\n              :label=\"subject.name\"\n              :value=\"subject.id\"/>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"课程标题\" prop=\"title\">\n          <el-input v-model=\"eduCourse.title\"/>\n        </el-form-item>\n        <el-form-item label=\"课程价格\" prop=\"price\">\n          <el-input-number :min=\"0\" v-model=\"eduCourse.price\" controls-position=\"right\"/>\n        </el-form-item>\n        <el-form-item label=\"总课时\" prop=\"lessonNum\">\n          <el-input-number :min=\"0\" v-model=\"eduCourse.lessonNum\" controls-position=\"right\"/>\n        </el-form-item>\n        <el-form-item label=\"课程封面\" prop=\"cover\">\n          <el-upload\n            :show-file-list=\"false\"\n            action=\"#\"\n            class=\"avatar-uploader\"\n            :http-request=\"uploadLogo\">\n            <img :src=\"eduCourse.cover\" width=\"380\" height=\"380\">\n          </el-upload>\n        </el-form-item>\n        <el-form-item label=\"是否发布\" prop=\"status\">\n          <el-select v-model=\"eduCourse.status\" placeholder=\"请选择\">\n            <el-option label=\"是\" value=\"Normal\"></el-option>\n            <el-option label=\"否\" value=\"Draft\"></el-option>\n          </el-select>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import api from '@/api/business/eduCourse'\n    import subject from '@/api/business/eduSubject'\n    import oss from '@/api/business/oss'\n    const defaultForm = {}\n    export default {\n        data() {\n            return {\n                listLoading: true, // 数据是否正在加载\n                list: null, // banner列表\n                total: 0, // 数据库中的总记录数\n                page: 1, // 默认页码\n                limit: 10, // 每页记录数\n                searchObj: {}, // 查询表单对象\n                dialogVisible: false,\n                eduCourse: {\n                    cover: 'https://lanf001.oss-cn-beijing.aliyuncs.com/2023/03/26/1ada44965a134935ba49d02cf9dcda71file.png'\n                },\n                saveBtnDisabled: false,\n                BASE_API: process.env.VUE_APP_BASE_API, //获取dev.env.js里面地\n                teacherList: [],//封装所有的讲师\n                subjectOneList: [],//一级分类\n                subjectTwoList: []//二级分类\n            }\n        },\n        // 生命周期函数：内存准备完毕，页面尚未渲染\n        created() {\n            console.log('list created......')\n            this.fetchData()\n        },\n        // 生命周期函数：内存准备完毕，页面渲染成功\n        mounted() {\n            console.log('list mounted......')\n            //初始化所有讲师\n            this.getListTeacher()\n            //初始化一级分类\n            this.getOneSubject()\n        },\n        methods: {\n            uploadLogo(param) {\n                // 获取上传的文件名\n                var file = param.file\n                const isJPG = file.type === 'image/jpeg'\n                const isJPG2 = file.type === 'image/png'\n                const isLt2M = file.size / 1024 / 1024 < 2\n                if (!isJPG && !isJPG2) {\n                    this.$message.error('上传头像图片只能是 JPG 或者png 格式!')\n                    return\n                }\n                if (!isLt2M) {\n                    this.$message.error('上传头像图片大小不能超过 2MB!')\n                    return\n                }\n                //发送请求的参数格式为FormData\n                const formData = new FormData();\n                formData.append(\"file\", file)\n                oss.uploadFile(formData)\n                    .then((res) => {\n                        if (res.code == 200) {\n                            this.eduCourse.cover = res.data.url\n                        }\n                        //this.refs.upload.clearFiles();\n                    })\n                    .catch((err) => {\n\n                    });\n            },\n            //点击某个一级分类，触发change，显示对应二级分类\n            subjectLevelOneChanged(value) {\n                //value就是一级分类id值\n                //遍历所有的分类，包含一级和二级\n                for (var i = 0; i < this.subjectOneList.length; i++) {\n                    //每个一级分类\n                    var oneSubject = this.subjectOneList[i]\n                    //判断：所有一级分类id 和 点击一级分类id是否一样\n                    if (value === oneSubject.id) {\n                        //从一级分类获取里面所有的二级分类\n                        this.subjectTwoList = oneSubject.children\n                        //把二级分类id值清空\n                        this.eduCourse.subjectId = ''\n                    }\n                }\n                this.$forceUpdate()\n            },\n            //查询所有的一级分类\n            getOneSubject() {\n                subject.findNodes()\n                    .then(response => {\n                        this.subjectOneList = response.data\n                    })\n            },\n            //查询所有的讲师\n            getListTeacher() {\n                api.getListTeacher()\n                    .then(response => {\n                        this.teacherList = response.data\n                    })\n            },\n            // 加载banner列表数据\n            fetchData(page = 1) {\n                debugger\n                this.page = page\n                api.getPageList(this.page, this.limit, this.searchObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.list = response.data.records\n                        this.total = response.data.total\n\n                        // 数据加载并绑定成功\n                        this.listLoading = false\n                    }\n                )\n            },\n            // 重置查询表单\n            resetData() {\n                console.log('重置查询表单')\n                this.searchObj = {}\n                this.fetchData()\n            },\n            // 根据id删除数据\n            removeDataById(id) {\n                // debugger\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => { // promise\n                    // 点击确定，远程调用ajax\n                    return api.removeById(id)\n                }).then((response) => {\n                    this.fetchData(this.page)\n                    this.$message.success(response.message || '删除成功')\n                }).catch(() => {\n                    this.$message.info('取消删除')\n                })\n            },\n            //弹出添加表单\n            add() {\n                this.dialogVisible = true\n                this.eduCourse = {cover: 'https://lanf001.oss-cn-beijing.aliyuncs.com/2023/03/26/1ada44965a134935ba49d02cf9dcda71file.png'}\n            },\n            //编辑\n            edit(id) {\n                this.dialogVisible = true\n                api.getById(id).then(response => {\n                    this.subjectLevelOneChanged(response.data.subjectParentId)\n                    this.eduCourse = response.data\n                })\n            },\n            //添加或更新\n            saveOrUpdate() {\n                if (!this.eduCourse.id) {\n                    this.save()\n                } else {\n                    this.update()\n                }\n            },\n            //添加\n            save() {\n                api.save(this.eduCourse).then(response => {\n                    this.$message.success('操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            },\n            //更新\n            update() {\n                api.updateById(this.eduCourse).then(response => {\n                    this.$message.success(response.message || '操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            }\n        }\n    }\n</script>\n"]}]}
