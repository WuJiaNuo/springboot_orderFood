{"remainingRequest":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\exam_vue\\src\\views\\business\\eduChapter.vue?vue&type=script&lang=js&","dependencies":[{"path":"F:\\exam_vue\\src\\views\\business\\eduChapter.vue","mtime":1681196641785},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1655715099000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["eduChapter.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"eduChapter.vue","sourceRoot":"src/views/business","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search-div\">\n      <el-form label-width=\"70px\" size=\"small\">\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"章节名称\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.title\" placeholder=\"章节名称\"></el-input>\n            </el-form-item>\n          </el-col>\n          <el-col :span=\"8\">\n            <el-form-item label=\"所属课程\">\n              <el-select\n                v-model=\"searchObj.courseId\"\n                placeholder=\"课程\">\n                <el-option\n                  v-for=\"course in courseList\"\n                  :key=\"course.id\"\n                  :label=\"course.title\"\n                  :value=\"course.id\"/>\n              </el-select>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row style=\"display:flex\">\n          <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\n        </el-row>\n      </el-form>\n    </div>\n    <!-- 工具条 -->\n    <div class=\"tools-div\">\n      <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\">添 加</el-button>\n    </div>\n    <!-- 列表 -->\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      stripe\n      border\n      style=\"width: 100%;margin-top: 10px;\">\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"courseName\" label=\"课程名称\"/>\n      <el-table-column prop=\"title\" label=\"章节名称\"/>\n      <el-table-column prop=\"sort\" label=\"显示排序\"/>\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页组件 -->\n    <el-pagination\n      :current-page=\"page\"\n      :total=\"total\"\n      :page-size=\"limit\"\n      style=\"padding: 30px 0; text-align: center;\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"fetchData\"\n    />\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"eduChapter\" label-width=\"100px\" size=\"small\" style=\"padding-right: 40px;\">\n        <el-form-item label=\"所属课程\">\n<!--          <el-select-->\n<!--            v-model=\"eduChapter.courseId\"-->\n<!--            placeholder=\"课程\">-->\n<!--            <el-option-->\n<!--              v-for=\"course in courseList\"-->\n<!--              :key=\"course.id\"-->\n<!--              :label=\"course.title\"-->\n<!--              :value=\"course.id\"/>-->\n<!--          </el-select>-->\n          <course-select\n            :value=\"eduChapter.courseId\"\n            @getInfo=\"getCourse\" :single=\"true\" >\n\n          </course-select>\n        </el-form-item>\n        <el-form-item label=\"章节名称\" prop=\"title\">\n          <el-input v-model=\"eduChapter.title\"/>\n        </el-form-item>\n        <el-form-item label=\"显示排序\" prop=\"sort\">\n          <el-input-number :min=\"0\" v-model=\"eduChapter.sort\" controls-position=\"right\"/>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import api from '@/api/business/eduChapter'\n    import coursea from '@/api/business/eduCourse'\n    import courseSelect from './eduChapterCourseSelect'\n    const defaultForm = {}\n    export default {\n        components:{\n            courseSelect\n        },\n        data() {\n            return {\n                listLoading: true, // 数据是否正在加载\n                list: null, // banner列表\n                total: 0, // 数据库中的总记录数\n                page: 1, // 默认页码\n                limit: 10, // 每页记录数\n                searchObj: {}, // 查询表单对象\n                dialogVisible: false,\n                eduChapter: defaultForm,\n                saveBtnDisabled: false,\n                courseList: [],\n            }\n        },\n        // 生命周期函数：内存准备完毕，页面尚未渲染\n        created() {\n            console.log('list created......')\n            this.fetchData()\n            this.getAllCourse()\n        },\n        // 生命周期函数：内存准备完毕，页面渲染成功\n        mounted() {\n            console.log('list mounted......')\n        },\n        methods: {\n            // 关联课程\n            getCourse(selections) {\n                this.eduChapter.courseId = selections.map(item => item.id).join(',')\n            },\n            // 加载banner列表数据\n            fetchData(page = 1) {\n                debugger\n                this.page = page\n                api.getPageList(this.page, this.limit, this.searchObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.list = response.data.records\n                        this.total = response.data.total\n\n                        // 数据加载并绑定成功\n                        this.listLoading = false\n                    }\n                )\n            },\n            //查询所有的课程\n            getAllCourse() {\n                coursea.getAllCourse()\n                    .then(response => {\n                        this.courseList = response.data\n                    })\n            },\n            // 重置查询表单\n            resetData() {\n                console.log('重置查询表单')\n                this.searchObj = {}\n                this.fetchData()\n            },\n            // 根据id删除数据\n            removeDataById(id) {\n                // debugger\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => { // promise\n                    // 点击确定，远程调用ajax\n                    return api.removeById(id)\n                }).then((response) => {\n                    this.fetchData(this.page)\n                    this.$message.success(response.message || '删除成功')\n                }).catch(() => {\n                    this.$message.info('取消删除')\n                })\n            },\n            //弹出添加表单\n            add() {\n                this.dialogVisible = true\n                this.eduChapter = {}\n            },\n            //编辑\n            edit(id) {\n                this.dialogVisible = true\n                api.getById(id).then(response => {\n                    this.eduChapter = response.data\n                })\n            },\n            //添加或更新\n            saveOrUpdate() {\n                if (!this.eduChapter.id) {\n                    this.save()\n                } else {\n                    this.update()\n                }\n            },\n            //添加\n            save() {\n                api.save(this.eduChapter).then(response => {\n                    this.$message.success('操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            },\n            //更新\n            update() {\n                api.updateById(this.eduChapter).then(response => {\n                    this.$message.success(response.message || '操作成功')\n                    this.dialogVisible = false\n                    this.fetchData(this.page)\n                })\n            }\n        }\n    }\n</script>\n"]}]}
