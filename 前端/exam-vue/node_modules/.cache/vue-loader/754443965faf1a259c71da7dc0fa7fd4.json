{"remainingRequest":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\exam_vue\\src\\views\\business\\eduChapterCourseSelectDialog.vue?vue&type=style&index=0&id=32b724fb&lang=scss&","dependencies":[{"path":"F:\\exam_vue\\src\\views\\business\\eduChapterCourseSelectDialog.vue","mtime":1681200324499},{"path":"F:\\exam_vue\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1655715156000},{"path":"F:\\exam_vue\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1655715099000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["eduChapterCourseSelectDialog.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0MA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"eduChapterCourseSelectDialog.vue","sourceRoot":"src/views/business","sourcesContent":["<template>\n  <div>\n    <el-dialog\n      title=\"课程选择\"\n      width=\"1000px\"\n      :close-on-click-modal=\"false\"\n      :append-to-body=\"true\"\n      v-dialogDrag\n      class=\"userDialog\"\n      :visible.sync=\"visible\">\n      <el-container style=\"height: 500px\">\n        <el-container>\n          <div class=\"search-div\">\n            <el-form\n              size=\"small\"\n              :inline=\"true\"\n              ref=\"searchForm\"\n              :model=\"searchForm\"\n              @keyup.enter.native=\"refreshList()\"\n              @submit.native.prevent>\n              <el-form-item prop=\"title\">\n                <el-input\n                  size=\"small\"\n                  v-model=\"searchForm.title\"\n                  placeholder=\"课程名称\"\n                  clearable\n                ></el-input>\n              </el-form-item>\n              <el-form-item prop=\"price\">\n                <el-input\n                  size=\"small\"\n                  v-model=\"searchForm.price\"\n                  placeholder=\"课程价格\"\n                  clearable\n                ></el-input>\n              </el-form-item>\n              <el-form-item>\n                <el-button\n                  type=\"primary\"\n                  @click=\"refreshList()\"\n                  size=\"small\"\n                  icon=\"el-icon-search\">查询\n                </el-button>\n                <el-button\n                  @click=\"resetSearch()\"\n                  size=\"small\"\n                  icon=\"el-icon-refresh-right\">重置\n                </el-button>\n              </el-form-item>\n            </el-form>\n          </div>\n                <el-table\n                  :data=\"dataList\"\n                  v-loading=\"loading\"\n                  size=\"small\"\n                  border\n                  ref=\"courseTable\"\n                  @select=\"handleSelectionChange\"\n                  height=\"calc(100% - 40px)\"\n                  style=\"width: 100%\"\n                >\n                <el-table-column\n                  type=\"selection\"\n                  header-align=\"center\"\n                  align=\"center\"\n                  width=\"50\"\n                >\n                </el-table-column>\n                <el-table-column\n                  prop=\"title\"\n                  header-align=\"center\"\n                  align=\"left\"\n                  sortable=\"custom\"\n                  min-width=\"90\"\n                  label=\"课程名称\"\n                >\n                </el-table-column>\n                <el-table-column\n                  prop=\"price\"\n                  header-align=\"center\"\n                  align=\"left\"\n                  sortable=\"custom\"\n                  min-width=\"110\"\n                  label=\"价格\"\n                >\n                </el-table-column>\n              </el-table>\n              <el-pagination\n                @size-change=\"sizeChangeHandle\"\n                @current-change=\"currentChangeHandle\"\n                :current-page=\"pageNo\"\n                :page-sizes=\"[5, 10, 50, 100]\"\n                :page-size=\"pageSize\"\n                :total=\"total\"\n                layout=\"total, sizes, prev, pager, next, jumper\"\n              >\n              </el-pagination>\n\n        </el-container>\n      </el-container>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button\n          size=\"small\"\n          @click=\"visible = false\"\n          icon=\"el-icon-circle-close\"\n        >关闭</el-button>\n        <el-button\n          size=\"small\"\n          type=\"primary\"\n          icon=\"el-icon-circle-check\"\n          @click=\"doSubmit()\"\n        >确定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import courseApi from '@/api/business/eduCourse'\n\n    export default {\n        data() {\n            return {\n                searchForm: {\n                    title: '',\n                    price: ''\n                },\n                dataListAllSelections: [], // 所有选中的数据包含跨页数据\n                idKey: \"id\", // 标识列表数据中每一行的唯一键的名称(需要按自己的数据改一下)\n                dataList: [],\n                pageNo: 1,\n                pageSize: 10,\n                total: 0,\n                loading: false,\n                visible: false,\n            };\n        },\n        props: {\n            selectData: {\n                type: Array,\n                default: () => {\n                    return [];\n                },\n            },\n            // 是否启用单选\n            single: {\n                type: Boolean,\n                default: false\n            }\n        },\n        methods: {\n            init() {\n                this.visible = true;\n                this.$nextTick(() => {\n                    this.dataListAllSelections = JSON.parse(JSON.stringify(this.selectData));\n                    this.resetSearch();\n                });\n            },\n            // 获取数据列表\n            refreshList() {\n                this.loading = true;\n                courseApi.getPageList(this.pageNo, this.pageSize, this.searchForm).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.dataList = response.data.records\n                        this.total = response.data.total\n                        // 数据加载并绑定成功\n                        this.loading = false\n                    }\n                )\n            },\n            // 每页数\n            sizeChangeHandle(val) {\n                this.pageSize = val;\n                this.pageNo = 1;\n                this.refreshList();\n            },\n            // 当前页\n            currentChangeHandle(val) {\n                this.pageNo = val;\n                this.refreshList();\n            },\n            // 排序\n            resetSearch() {\n                this.$refs.searchForm.resetFields();\n                this.refreshList();\n            },\n            // 选中数据\n            handleSelectionChange(selection, row) {\n                if (this.single && selection.length > 1) {\n                    this.$refs.courseTable.clearSelection();\n                    this.$refs.courseTable.toggleRowSelection(row);\n                }\n                this.dataListAllSelections = this.single ? [row] : selection\n            },\n            doSubmit() {\n                this.visible = false;\n                this.$emit(\"doSubmit\", this.dataListAllSelections);\n            },\n        },\n    };\n</script>\n<style lang=\"scss\">\n  .userDialog {\n    .el-dialog__body {\n      padding: 10px 0px 0px 10px;\n      color: #606266;\n      font-size: 14px;\n      word-break: break-all;\n    }\n\n    .el-main {\n      padding: 20px 20px 5px 20px;\n\n      .el-pagination {\n        margin-top: 5px;\n      }\n    }\n  }\n</style>\n"]}]}
