{"remainingRequest":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\exam_vue\\src\\views\\business\\eduChapterCourseSelect.vue?vue&type=style&index=0&id=11e1705a&lang=css&","dependencies":[{"path":"F:\\exam_vue\\src\\views\\business\\eduChapterCourseSelect.vue","mtime":1681195807734},{"path":"F:\\exam_vue\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1655715156000},{"path":"F:\\exam_vue\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1655715099000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["eduChapterCourseSelect.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6FA;AACA;AACA;AACA;AACA;AACA;AACA","file":"eduChapterCourseSelect.vue","sourceRoot":"src/views/business","sourcesContent":["<template>\n  <div>\n    <el-input placeholder=\"请选择\" :size=\"size\" :disabled=\"inpDisabled\" style=\"line-hight:40px\" v-model=\"name\" class=\"input-with-select\">\n      <el-button slot=\"append\" :disabled=\"btnDisabled\" @click=\"showUserSelect\" icon=\"el-icon-search\"></el-button>\n    </el-input>\n    <!-- 课程列表 -->\n    <CourseSelectDialog\n      ref=\"courseSelect\"\n      @doSubmit=\"selectionsToInput\"\n      :selectData=\"selectData\"\n      :single=\"single\"\n    />\n  </div>\n</template>\n<script>\n    import CourseSelectDialog from './eduChapterCourseSelectDialog'\n    import courseApi from '@/api/business/eduCourse'\n    export default {\n        data () {\n            return {\n                name: '',\n                selectData: [],\n                courseService: null\n            }\n        },\n        props: {\n            size: {\n                type: String,\n                default: 'small'\n            },\n            value: {\n                type: String,\n                default: ''\n            },\n            btnDisabled: {\n                type: Boolean,\n                default: false\n            },\n            inpDisabled: {\n                type: Boolean,\n                default: true\n            },\n            single: {  // 是否启用单选\n                type: Boolean,\n                default: false\n            }\n        },\n        components: {\n            CourseSelectDialog\n        },\n        created () {\n        },\n        watch: {\n            value: {\n                handler (newVal) {\n                    this.selectData = []\n                    if (newVal) {\n                        newVal.split(',').forEach((id) => { // 回显拿数据\n                            courseApi.getById(id).then(\n                                response => {\n                                    if (response && response.data && response.data.id !== '') {\n                                        this.selectData.push(response.data)\n                                    }\n                                }\n                            )\n                        })\n                    }\n                },\n                immediate: true,\n                deep: false\n            },\n            selectData: {\n                handler (newVal) {\n                    this.name = newVal.map(course => course.title).join(',')\n                },\n                immediate: false,\n                deep: false\n            }\n        },\n        methods: {\n            // 设置选中\n            selectionsToInput (selections) {\n                this.selectData = selections\n                this.$emit('getInfo', this.selectData)\n            },\n            // 显示列表\n            showUserSelect () {\n                this.$refs.courseSelect.init()\n            }\n        }\n    }\n</script>\n<style>\n  .el-form-item__content .el-input-group {\n    vertical-align: middle;\n  }\n  .el-tag + .el-tag {\n    margin-left: 5px;\n    margin-bottom: 5px;\n  }\n</style>\n"]}]}
