rt default {\r\n        data () {\r\n            return {\r\n                name: '',\r\n                selectData: [],\r\n                classesService: null\r\n            }\r\n        },\r\n        props: {\r\n            size: {\r\n                type: String,\r\n                default: 'small'\r\n            },\r\n            value: {\r\n                type: String,\r\n                default: ''\r\n            },\r\n            btnDisabled: {\r\n                type: Boolean,\r\n                default: false\r\n            },\r\n            inpDisabled: {\r\n                type: Boolean,\r\n                default: true\r\n            },\r\n            single: {  // 是否启用单选\r\n                type: Boolean,\r\n                default: false\r\n            }\r\n        },\r\n        components: {\r\n            ClassesSelectDialog\r\n        },\r\n        created () {\r\n        },\r\n        watch: {\r\n            value: {\r\n                handler (newVal) {\r\n                    this.selectData = []\r\n                    if (newVal) {\r\n                        newVal.split(',').forEach((id) => { // 回显拿数据\r\n                            classesApi.getById(id).then(\r\n                                response => {\r\n                                    if (response && response.data && response.data.id !== '') {\r\n                                        this.selectData.push(response.data)\r\n                                    }\r\n                                }\r\n                            )\r\n                        })\r\n                    }\r\n                },\r\n                immediate: true,\r\n                deep: false\r\n            },\r\n            selectData: {\r\n                handler (newVal) {\r\n                    this.name = newVal.map(item => item.name).join(',')\r\n                },\r\n                immediate: false,\r\n                deep: false\r\n            }\r\n        },\r\n        methods: {\r\n          