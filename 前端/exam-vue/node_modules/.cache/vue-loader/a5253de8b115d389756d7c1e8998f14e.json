{"remainingRequest":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!F:\\exam_vue\\src\\views\\business\\eduVideo.vue?vue&type=script&lang=js&","dependencies":[{"path":"F:\\exam_vue\\src\\views\\business\\eduVideo.vue","mtime":1680353242465},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\babel-loader\\lib\\index.js","mtime":315532800000},{"path":"F:\\exam_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"F:\\exam_vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1655715099000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["eduVideo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4HA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"eduVideo.vue","sourceRoot":"src/views/business","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"search-div\">\n      <el-form label-width=\"70px\" size=\"small\">\n        <el-row>\n          <el-col :span=\"8\">\n            <el-form-item label=\"节点名称\">\n              <el-input style=\"width: 95%\" v-model=\"searchObj.title\" placeholder=\"节点名称\"></el-input>\n            </el-form-item>\n          </el-col>\n        </el-row>\n        <el-row style=\"display:flex\">\n          <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"fetchData()\">搜索</el-button>\n          <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetData\">重置</el-button>\n        </el-row>\n      </el-form>\n    </div>\n    <!-- 工具条 -->\n    <div class=\"tools-div\">\n      <el-button type=\"success\" icon=\"el-icon-plus\" size=\"mini\" @click=\"add\">添 加</el-button>\n    </div>\n    <!-- 列表 -->\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      stripe\n      border\n      style=\"width: 100%;margin-top: 10px;\">\n      <el-table-column\n        label=\"序号\"\n        width=\"70\"\n        align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ (page - 1) * limit + scope.$index + 1 }}\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"courseName\" label=\"课程名称\"/>\n      <el-table-column prop=\"chapterName\" label=\"章节名称\"/>\n      <el-table-column prop=\"title\" label=\"节点名称\"/>\n      <el-table-column prop=\"sort\" label=\"排序字段\"/>\n      <el-table-column prop=\"playCount\" label=\"播放次数\"/>\n      <el-table-column prop=\"isFree\" label=\"是否可以试听：0收费 1免费\"/>\n      <el-table-column prop=\"status\" label=\"视频状态\"/>\n      <el-table-column prop=\"createTime\" label=\"创建时间\"/>\n      <el-table-column label=\"操作\" align=\"center\" fixed=\"right\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"edit(scope.row.id)\" title=\"修改\"/>\n          <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeDataById(scope.row.id)\" title=\"删除\"/>\n        </template>\n      </el-table-column>\n    </el-table>\n    <!-- 分页组件 -->\n    <el-pagination\n      :current-page=\"page\"\n      :total=\"total\"\n      :page-size=\"limit\"\n      style=\"padding: 30px 0; text-align: center;\"\n      layout=\"total, prev, pager, next, jumper\"\n      @current-change=\"fetchData\"\n    />\n    <el-dialog title=\"添加/修改\" :visible.sync=\"dialogVisible\" :close-on-click-modal=\"false\" width=\"40%\">\n      <el-form ref=\"dataForm\" :model=\"eduVideo\" label-width=\"100px\" size=\"small\" style=\"padding-right: 40px;\">\n        <el-form-item label=\"所属课程\">\n          <el-select\n            v-model=\"eduVideo.courseId\"\n            placeholder=\"课程\" @change=\"getChapterByCourse\">\n            <el-option\n              v-for=\"course in courseList\"\n              :key=\"course.id\"\n              :label=\"course.title\"\n              :value=\"course.id\"/>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"所属章节\">\n          <el-select v-model=\"eduVideo.chapterId\" @change=\"$forceUpdate()\" placeholder=\"所属章节\">\n            <el-option\n              v-for=\"chapter in chapterList\"\n              :key=\"chapter.id\"\n              :label=\"chapter.title\"\n              :value=\"chapter.id\"/>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"节点名称\" prop=\"title\">\n          <el-input v-model=\"eduVideo.title\"/>\n        </el-form-item>\n        <el-form-item label=\"上传视频\" prop=\"upload\">\n          <el-upload\n            class=\"upload-demo\"\n            :action=\"BASE_API+'/eduvod/video/uploadAlyiVideo'\"\n            :on-preview=\"handlePreview\"\n            :on-remove=\"handleRemove\"\n            :on-success=\"uploadBack\"\n            :before-remove=\"beforeRemove\"\n            multiple\n            :limit=\"1\"\n            ref=\"upload\"\n            :on-exceed=\"handleExceed\"\n            :headers=\"headers\"\n            :show-file-list=\"false\"\n            :on-progress=\"uploading\"\n            :file-list=\"fileList\">\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\n          </el-upload>\n          &nbsp;&nbsp;\n          <el-button type=\"danger\" size=\"small\" @click=\"clearUpload\">删除</el-button>\n          <el-progress :percentage=\"percentage\"></el-progress>\n          <div>{{eduVideo.videoOriginalName}}</div>\n        </el-form-item>\n        <el-form-item label=\"排序\" prop=\"sort\">\n          <el-input-number :min=\"0\" v-model=\"eduVideo.sort\" controls-position=\"right\"/>\n        </el-form-item>\n        <el-form-item label=\"是否可以试听\" prop=\"isFree\">\n          <el-radio v-model=\"eduVideo.isFree\" :label=\"1\">是</el-radio>\n          <el-radio v-model=\"eduVideo.isFree\" :label=\"0\">否</el-radio>\n        </el-form-item>\n      </el-form>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\" size=\"small\" icon=\"el-icon-refresh-right\">取 消</el-button>\n        <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"saveOrUpdate()\" size=\"small\">确 定</el-button>\n      </span>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import api from '@/api/business/eduVideo'\n    import courseApi from '@/api/business/eduCourse'\n    import chapterApi from '@/api/business/eduChapter'\n    import {getToken} from '@/utils/auth'\n    const defaultForm = {\n        videoSourceId: '',\n        videoOriginalName: ''\n    }\n    export default {\n        data() {\n            return {\n                listLoading: true, // 数据是否正在加载\n                list: null, // banner列表\n                total: 0, // 数据库中的总记录数\n                page: 1, // 默认页码\n                limit: 10, // 每页记录数\n                searchObj: {}, // 查询表单对象\n                dialogVisible: false,\n                eduVideo: defaultForm,\n                saveBtnDisabled: false,\n                courseList: [],//课程列表\n                chapterList: [],//章节列表\n                fileList: [],//文件列表\n                percentage: 0,//上传文件进度\n                headers: {token: getToken()},\n                BASE_API: process.env.VUE_APP_BASE_API //获取dev.env.js里面地\n            }\n        },\n        // 生命周期函数：内存准备完毕，页面尚未渲染\n        created() {\n            console.log('list created......')\n            this.fetchData()\n            this.getCourseList()\n        },\n        // 生命周期函数：内存准备完毕，页面渲染成功\n        mounted() {\n            console.log('list mounted......')\n        },\n        methods: {\n            //上传begin\n            handleRemove(file, fileList) {\n                console.log(file, fileList);\n            },\n            handlePreview(file) {\n                console.log(file);\n            },\n            handleExceed(files, fileList) {\n                this.$message.warning(`只能选择一个文件`);\n            },\n            beforeRemove(file, fileList) {\n                return this.$confirm(`确定移除 ${file.name}？`);\n            },\n            uploading(file, fileList) {\n                const percent = parseInt(file.percent);\n                this.percentage = percent > 98 ? 98 : percent;\n            },\n            uploadBack(response, file) {\n                if (response.code == '200') {\n                    this.percentage = 100\n                    this.eduVideo.videoOriginalName = file.name\n                    this.$message.success('视频上传成功')\n                    this.eduVideo.videoSourceId = response.data\n                } else {\n                    this.$message.error('系统异常，请联系管理员')\n                }\n            },\n            clearUpload() {\n                this.percentage = 0\n                this.eduVideo.videoOriginalName = ''\n                this.fileList = []\n            },\n            //上传end\n            // 加载banner列表数据\n            fetchData(page = 1) {\n                debugger\n                this.page = page\n                api.getPageList(this.page, this.limit, this.searchObj).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.list = response.data.records\n                        this.total = response.data.total\n                        // 数据加载并绑定成功\n                        this.listLoading = false\n                    }\n                )\n            },\n            getChapterByCourse(value, isEdit) {\n                chapterApi.findChapterByCourse(value)\n                    .then(response => {\n                        this.chapterList = response.data\n                        if (!isEdit) {\n                            this.eduVideo.chapterId = ''\n                        }\n                        this.$forceUpdate()\n                    })\n            },\n            //查询所有的课程\n            getCourseList() {\n                courseApi.getAllCourse()\n                    .then(response => {\n                        this.courseList = response.data\n                    })\n            },\n            // 重置查询表单\n            resetData() {\n                console.log('重置查询表单')\n                this.searchObj = {}\n                this.fetchData()\n            },\n            // 根据id删除数据\n            removeDataById(id) {\n                // debugger\n                this.$confirm('此操作将永久删除该记录, 是否继续?', '提示', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(() => { // promise\n                    // 点击确定，远程调用ajax\n                    return api.removeById(id)\n                }).then((response) => {\n                    this.fetchData(this.page)\n                    this.$message.success(response.message || '删除成功')\n                }).catch(() => {\n                    this.$message.info('取消删除')\n                })\n            },\n            //弹出添加表单\n            add() {\n                this.dialogVisible = true\n                this.eduVideo = {}\n            },\n            //编辑\n            edit(id) {\n                this.dialogVisible = true\n                api.getById(id).then(response => {\n                    this.eduVideo = response.data\n                    this.getChapterByCourse(this.eduVideo.courseId, true)\n                })\n            },\n            //添加或更新\n            saveOrUpdate() {\n                if (!this.eduVideo.id) {\n                    this.save()\n                } else {\n                    this.update()\n                }\n            },\n            //添加\n            save() {\n                api.save(this.eduVideo).then(response => {\n                    this.$message.success('操作成功')\n                    this.dialogVisible = false\n                    this.percentage = 0\n                    clearUpload()\n                    this.fetchData(this.page)\n\n                })\n            },\n            //更新\n            update() {\n                api.updateById(this.eduVideo).then(response => {\n                    this.$message.success(response.message || '操作成功')\n                    this.dialogVisible = false\n                    this.percentage = 0\n                    clearUpload()\n                    this.fetchData(this.page)\n                })\n            }\n        }\n    }\n</script>\n"]}]}
