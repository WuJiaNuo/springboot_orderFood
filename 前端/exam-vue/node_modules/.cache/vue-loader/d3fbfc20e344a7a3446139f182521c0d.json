pan=\"5\">\n                <el-form-item prop=\"dicCode\">\n                  <el-input\n                    size=\"small\"\n                    v-model=\"searchForm.dicCode\"\n                    placeholder=\"所属分类\"\n                    @input=\"changeInput($event)\"\n                    clearable>\n                  </el-input>\n                </el-form-item>\n              </el-col>\n              <el-col :span=\"5\">\n                <el-form-item prop=\"code\">\n                  <el-input\n                    size=\"small\"\n                    v-model=\"searchForm.code\"\n                    placeholder=\"编码\"\n                    @input=\"changeInput($event)\"\n                    clearable>\n                  </el-input>\n                </el-form-item>\n              </el-col>\n              <el-col :span=\"5\">\n                <el-form-item prop=\"name\">\n                  <el-input\n                    size=\"small\"\n                    v-model=\"searchForm.name\"\n                    placeholder=\"名称\"\n                    @input=\"changeInput($event)\"\n                    clearable>\n                  </el-input>\n                </el-form-item>\n              </el-col>\n              <el-col :span=\"5\">\n                <el-form-item>\n                  <el-button\n                    type=\"primary\"\n                    @click=\"refreshList()\"\n                    size=\"small\"\n                    icon=\"el-icon-search\">搜索\n                  </el-button>\n                  <el-button\n                    @click=\"resetSearch()\"\n                    size=\"small\"\n                    icon=\"el-icon-refresh-right\">重置\n                  </el-button>\n                </el-form-item>\n              </el-col>\n            </el-row>\n          </el-form>\n        </div>\n        <el-table\n          :data=\"dataList\"\n          v-loading=\"loading\"\n          size=\"small\"\n          border\n          ref=\"sysDicItemTable\"\n          style=\"width: 100%;margin-top: 10px;\">\n          <el-table-column sortable prop=\"dicCode\" label=\"所属分类\"/>\n          <el-table-column sortable prop=\"code\" label=\"编码\"/>\n          <el-table-column sortable prop=\"name\" label=\"名称\"/>\n          <el-table-column sortable prop=\"createTime\" label=\"创建时间\"/>\n          <el-table-column sortable prop=\"updateTime\" label=\"修改时间\"/>\n        </el-table>\n        <el-pagination\n          @size-change=\"sizeChangeHandle\"\n          @current-change=\"currentChangeHandle\"\n          :current-page=\"pageNo\"\n          :page-sizes=\"[5, 10, 50, 100]\"\n          :page-size=\"pageSize\"\n          :total=\"total\"\n          style=\"padding: 30px 0; text-align: center;\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n        >\n        </el-pagination>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n<script>\n    import sysDicItemApi from '@/api/system/sysDicItem'\n    export default {\n        data() {\n            return {\n                searchForm: {},\n                idKey: \"id\", // 标识列表数据中每一行的唯一键的名称(需要按自己的数据改一下)\n                dataList: [],\n                pageNo: 1,\n                pageSize: 5,\n                total: 0,\n                loading: false,\n                visible: false,\n            };\n        },\n        props: {\n            dicCodes: {\n                type: String,\n                default: ''\n            }\n        },\n        methods: {\n            changeInput() {\n                this.$forceUpdate();\n            },\n            init() {\n                this.visible = true;\n                this.$nextTick(() => {\n                    this.resetSearch();\n                });\n            },\n            // 获取数据列表\n            refreshList() {\n                this.loading = true;\n                this.searchForm.dicCodes=this.dicCodes\n                sysDicItemApi.getPageList(this.pageNo, this.pageSize, this.searchForm).then(\n                    response => {\n                        //this.list = response.data.list\n                        this.dataList = response.data.records\n                        this.total = response.data.total\n                        // 数据加载并绑定成功\n                        this.loading = false\n                    }\n                )\n            },\n            // 每页数\n            sizeChangeHandle(val) {\n                this.pageSize = val;\n                this.pageNo = 1;\n                this.refreshList();\n            },\n            // 当前页\n            currentChangeHandle(val) {\n                this.pageNo = val;\n                this.refreshList();\n            },\n            // 排序\n            resetSearch() {\n                this.$refs.searchForm.resetFields();\n                this.refreshList();\n            }\n        },\n    };\n</script>\n<style lang=\"scss\">\n</style>\n"]}]}
